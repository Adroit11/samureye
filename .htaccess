
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /samureye/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /samureye/index.php [L]
</IfModule>

# END WordPress

Options -Indexes

<Files .htaccess>
order allow,deny
deny from all
</Files>
